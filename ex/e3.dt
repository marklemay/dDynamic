

data Unit : * {
  | tt  : Unit
};

data Nat : * {
  | Z : Nat
  | S : Nat -> Nat
};


-- length indexed lists
data Vec : Nat -> * {
  | Nil  : Vec 0
  | Cons : Unit -> (n : Nat) -> Vec n -> Vec (S n)
};


head : Vec 1 -> Unit ;
head v = 
  case v <_ => Unit >{
   | (Cons a _ _) => a
   } ;


af : Unit ;
af = head (Cons tt 1 (Cons tt 0 Nil)) ;
-- cbv checks more then is strictly needed to get a result, whnf would result in ~tt

afok : Unit ;
afok = head (Cons tt 0 Nil) ;


data Bool : * {
  | true  : Bool
  | false : Bool
};

data Id : (A : *) -> A -> A -> * {
| refl  : (A : *) -> (a : A) -> Id A a a
};

badEq : Id (Bool -> Bool) (\x => true) (\x => false);
